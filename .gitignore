# Standard ignore patterns for Flutter projects

# Build artifacts
.dart_tool/
.flutter-plugins
.flutter-plugins-dependencies
.pub-cache/
.pub/
/build/
**/.packages
.metadata

# IDE-specific files
.idea/
.vscode/
*.iml
*.ipr
*.iws

# OS-specific files
.DS_Store
Thumbs.db

# Android-specific
android/.gradle/
android/local.properties
android/key.properties
android/app/debug/
android/app/profile/
android/app/release/

# iOS-specific
ios/.symlinks/
ios/.generated/

# Windows-specific
windows/build/

# Linux-specific
linux/build/

# macOS-specific
macos/build/

# Web-specific
web/build/

# Generated files
**/doc/api/
**/ios/Flutter/.last_build_id
app.*.symbols
app.*.map.json

# Project-specific temporary or generated files
~$*.docx # Temporary Word files
sa.rar # Archive, likely temporary
test.xml # Likely temporary test data
testH.html # Likely temporary test data
